{% extends 'admin.html.twig' %}

{% block title %}Pharmacist{% endblock %}

{% block contenu %}
    <h1>Pharmacist</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ pharmacist.id }}</td>
            </tr>
            <tr>
                <th>Certificate</th>
                <td>{{ pharmacist.certificate }}</td>
            </tr>
            <tr>
                <th>Pharmacy_name</th>
                <td>{{ pharmacist.pharmacyName }}</td>
            </tr>
            <tr>
                <th>Type</th>
                <td>{{ pharmacist.type }}</td>
            </tr>
            <tr>
                <th>Address</th>
                <td>{{ pharmacist.address }}</td>
            </tr>
            <tr>
                <th>Mobile</th>
                <td>{{ pharmacist.mobile }}</td>
            </tr>
            <tr>
                <th>Email</th>
                <td>{{ pharmacist.email }}</td>
            </tr>
            <tr>
                <th>Image</th>
                <td>{{ pharmacist.image }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('pharmacist_index') }}">back to list</a>

    <a href="{{ path('pharmacist_edit', {'id': pharmacist.id}) }}">edit</a>

    {{ include('backend/pharmacist/_delete_form.html.twig') }}
{% endblock %}
